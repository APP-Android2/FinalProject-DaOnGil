<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_navigation_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="kr.tekit.lion.daongil.presentation.login.fragment.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_selectInterestFragment"
            app:destination="@id/selectInterestFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectInterestFragment"
        android:name="kr.tekit.lion.daongil.presentation.login.fragment.SelectInterestFragment"
        android:label="fragment_select_interest"
        tools:layout="@layout/fragment_select_interest" >
        <action
            android:id="@+id/action_selectInterestFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="kr.tekit.lion.daongil.presentation.main.MainActivity"
        android:label="MainActivity" />
</navigation>